.depositBox{
  background-color #ffffff
}
.depositLi{
  display block
  background-color #ffffff
  .tbBox{
    padding 20px 34px 0px 34px
    ul{
      display block
      width 100%
      margin-bottom 10px
      li{
        display inline-block
        width 33.33333%
        vertical-align  top
        position relative
        &:nth-of-type(1){
          padding-left 15px
        }
        &:nth-of-type(3){
          padding-right 15px
        }
      }
    }
  }
}
.goNext{
  width 100%
  background-color #E5E5E5
  height 3px
  display inline-block
  vertical-align middle
  margin-top -5px
}
.startTime{
  position absolute
  left 0px
  top 0px
  img{
    width 13px
  }
}
.locking{
  display inline-block
  width 17px
  background-color #ffffff
  position absolute
  left 50%
  margin-left -8.5px
  text-align center
  img{
    width 13px
  }
}
.endTime{
  position absolute
  right  0px
  top 0px
  img{
    width 13px
  }
}
.tbText{
  padding 0px 15px
  border-bottom 1px #e5e5e5 solid
  ul{
    display block
    width 100%
    margin-bottom 20px
    li{
      display inline-block
      width 33.33333%
      vertical-align top
      font-size 12px
      color #666666
      &:nth-of-type(2){
        text-align center
      }
      &:nth-of-type(3){
        text-align right
      }
      .tbName{
        padding-bottom 6px
      }
      .timeText{
        font-size 11px
        color #AAAAAA
      }
    }
  }
}
.hx{
  display inline-block
  width 50%
  height 3px
  background-color #E5E5E5
  vertical-align middle
  margin-top -5px
}
.msBox{
  padding 15px
  background-color #ffffff
  p{
    font-size 14px
    color #AAAAAA
    &:nth-of-type(1){
      margin-bottom 10px
    }
  }
  .dbType{
    color #666
    float right
    text-align right
  }
}