.tablist {
  margin: 0;
  padding: 0;
  height: 45px;
  display: flex;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
   border-top:1px #b2b2b2 solid;
   z-index:999;
}

.tab {
  color: #fff;
  display: flex;
  flex: 1 1 auto;
  align-items: center;
  justify-content: center;
  background: #f8f8f8;
}

.tabpanel {
  margin: 0;
  margin-bottom: 45px;
}
.tabOne{
  font-size:12px;
  color:#444;
  text-align:center;
}

.tabIcon{
  display:inline-block;
  width:22px;
  height:22px;
 }
 .tabIcon1{
   background:url('../../assets/images/tab1.png');
    background-size:100% 100%; 
 }
