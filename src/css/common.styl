a {
  text-decoration none
}

.ml0 { 
  ml 0 
}

.mt15 { 
  mt 15
}

.mt30 {
  mt 30
}

.topBar {
  display flex
  pl 15
  pr 15
  mt 14
}

.topBarLeft {
  text-align left
  flex 1
  fs 16
  color #00a6e2
}

.topBarRight {
  text-align right
  flex 1
  fs 16
}

.markTitle {
  mt 34
  mb 40
  fs 25
  color #000
  text-align center
  display block
}

.panel {
  display flex
  flex-direction column
  width 100%
  height 100%
  bg #f0eff5
  pt 44
}

.panelContent {
  flex 1
  overflow-y scroll  
}

.buttonWrap {
  mt 40
  ml 15
  mr 15
}
  .loading{
    display inline-block
    width 100%;
    text-align center;
    color #ffffff;
    font-size 18px;
  }
  .loading>span{
    display inline-block;
  }
  .center{
    text-align center
  }
  .content{
    position absolute
    left 0px
    padding-top 44px
    top 0px
    width 100%
    height 100%
    background-color #ffffff
  }
.tip{
  left 0
}
