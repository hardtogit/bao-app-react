.container{
  background-color:#f1eff5;
  height:100%;
  position:absolute;
  .tip{
    font-size:14px;
    padding-left 15px;
    padding-right 15px
    color:#777;
    line-height:20px;
    margin 8px auto;
  }
  .bottom{
    margin 15px auto;

  }
  .textContent{
    padding-left 15px
    padding-top 10px
    padding-right 6px
    color #888
    font-size 14px
  }
  .protocol {
    color #00a6e2
    //margin 10px 15px 0
  }
  .rightBar{
    font-size: 16px;
    padding: 12px 0;
    margin-left: 15px;
    border-bottom: 1px solid #ddd;
    .rightIcon{
      float:right;
      .text{
        margin-right: 36px;
      }
      .arrow{
        position: relative;
        right: 10px;
        &:before,&:after{
          border: 10px solid transparent;
          border-left: 10px solid #fff;
          width: 0;
          height: 0;
          position: absolute;
          top: 0;
          right: 0;
          content: ' ';
        }
        &:before{
          border-left-color: #B6B6B6;
          right: -2px;

        }
      }
    }
  }
}