.root {
  position absolute
  width 100%
  height 100%
  left 0px
  top 0px
  overflow auto
  padding-top 44px
  bg $bg
  padding-bottom 50px
}
.timeBox{
  margin-top 10px
}
.endTime{
  position absolute
  right  0px
  top 0px
  img{
    width 13px
  }
}
.suJt{
  width: 10px;
  height: 20px;
  float: right;
  margin-top: -2px;
}
.depositBox{
  background-color #ffffff
  margin-top 10px
}
.pds{
  padding 0px 15px
}
.profit{
  padding 15px 0px
  border-bottom 1px #E5E5E5 solid
  p{
    padding 0px 15px
    font-size 16px
  }
}
.pdAll{
  padding 20px 17px
}
.iconUl{
  display block
  width 100%
  li{
    display inline-block
    width 33.33333%
    span{
      display block
      width 100%
      text-align center
      font-size 12px
      color #666666
    }
    img{
      width 30px
      margin-bottom 10px
    }
  }
}
.pdAll1{
  padding 15px
}
.hx{
  display inline-block
  width 50%
  height 3px
  background-color #E5E5E5
  vertical-align middle
  margin-top -5px
}
.modalStyle{
  position fixed
  z-index 999
}
.modalBody{
  position absolute
  bottom 0px
  padding-bottom 30px
  height auto
}
.suBox {
 padding: 10px 15px;
background-color: #fff;
margin-top: 10px;
font-size: 16px;
}
.listInfo{
  margin-top 10px;
  background-color #fff;
  padding-bottom:10px
  .item{
    padding-top:10px;
    display flex;
    .left{
      padding-left: 15px;
      color:#888;
      flex: 1
    }
    .right{
      padding-right: 15px;
      flex: 1;
      color:#777;
      text-align: right
    }
  }
}
.tip{
  margin-top: 30px;
  text-align:center;
  color:#aaa
}
